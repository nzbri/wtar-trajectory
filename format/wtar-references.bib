	journal = {Clin. Epidemiol},
	keywords = {Epidemiologic Methods, latent class, statistics, trajectories},
	language = {en},
	month = oct,
	pages = {1205--1222},
	shorttitle = {Trajectory modelling techniques useful to epidemiological research},
	title = {Trajectory modelling techniques useful to epidemiological research: a comparative narrative review of approaches},
